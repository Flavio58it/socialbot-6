<%= form_for(@product) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.submit "Create product" %>



<% end %>
